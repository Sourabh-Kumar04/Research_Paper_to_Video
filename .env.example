# Environment Configuration Template
# Copy this file to .env and fill in your actual values
# DO NOT commit .env files to version control!

# RASO Platform Configuration - Production

# Environment
RASO_ENV=production
RASO_LOG_LEVEL=your_value_here
RASO_DEBUG=false

# API Configuration
RASO_API_HOST=localhost
RASO_API_PORT=8000
RASO_API_WORKERS=your_value_here

# Database Configuration
RASO_DATABASE_URL=your_url_here
RASO_REDIS_PASSWORD=your_password_here

# Storage Configuration
RASO_DATA_PATH=your_value_here
RASO_TEMP_PATH=your_value_here
RASO_LOG_PATH=your_value_here
RASO_MAX_STORAGE_GB=your_value_here

# LLM Configuration - Google Gemini (Default)
RASO_LLM_PROVIDER=your_value_here
RASO_GOOGLE_API_KEY=your_api_key_here
RASO_GOOGLE_MODEL=your_value_here
RASO_GOOGLE_TEMPERATURE=your_value_here
RASO_GOOGLE_MAX_TOKENS=your_token_here

# Script Generation LLM
RASO_SCRIPT_LLM_PROVIDER=your_value_here
RASO_SCRIPT_LLM_MODEL=your_value_here

# Manim Code Generation LLM
RASO_MANIM_LLM_PROVIDER=your_value_here
RASO_MANIM_LLM_MODEL=your_value_here

# Content Analysis LLM
RASO_ANALYSIS_LLM_PROVIDER=your_value_here
RASO_ANALYSIS_LLM_MODEL=your_value_here

HUGGINGFACEHUB_API_TOKEN=your_token_here

# Backup LLM Configuration (Optional)
RASO_OPENAI_API_KEY=your_api_key_here
RASO_OPENAI_MODEL=your_value_here

# Animation Configuration
RASO_ANIMATION_RESOLUTION=your_value_here
RASO_ANIMATION_FPS=your_value_here
RASO_ANIMATION_QUALITY=your_value_here
RASO_MANIM_QUALITY=your_value_here

# Audio Configuration
RASO_TTS_PROVIDER=your_value_here
RASO_TTS_MODEL=your_value_here
RASO_AUDIO_SAMPLE_RATE=your_value_here
RASO_AUDIO_BITRATE=your_value_here

# Video Configuration
RASO_VIDEO_CODEC=your_value_here
RASO_VIDEO_BITRATE=your_value_here
RASO_VIDEO_PRESET=your_value_here
RASO_VIDEO_CRF=your_value_here

# System Configuration
RASO_MAX_CONCURRENT_JOBS=your_value_here
RASO_JOB_TIMEOUT_MINUTES=your_value_here
RASO_RETRY_ATTEMPTS=your_value_here
RASO_RETRY_DELAY_SECONDS=your_value_here

# Security Configuration
RASO_SECRET_KEY=your_api_key_here
RASO_CORS_ORIGINS=your_value_here
RASO_RATE_LIMIT_PER_MINUTE=your_value_here

# Monitoring Configuration
RASO_ENABLE_METRICS=your_value_here
RASO_METRICS_PORT=8000
RASO_HEALTH_CHECK_INTERVAL=your_value_here

# Production Features
RASO_ENABLE_CACHING=your_value_here
RASO_CACHE_TTL_SECONDS=your_value_here
RASO_ENABLE_COMPRESSION=your_value_here
RASO_ENABLE_SSL=your_value_here
